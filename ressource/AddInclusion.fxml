<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="500.0" maxWidth="530.0" prefHeight="450.0" prefWidth="530.0"
            stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="src.controller.AddInclusionController">
    <children>
        <GridPane prefHeight="450.0" prefWidth="530.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="289.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="addButton" mnemonicParsing="false" onAction="#addAction" prefHeight="50.0"
                        prefWidth="150.0" text="Ajouter +" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.rowIndex="6">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction" prefHeight="50.0"
                        prefWidth="150.0" text="Annuler" GridPane.halignment="CENTER" GridPane.rowIndex="6">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <TextField fx:id="idAnapathField" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="inclusionIDField" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <Label text="Identifiant d'inclusion :">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Date d'inclusion :" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Numéro anapath :" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Fichier référence PTFE 1 :" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="reference1FileButton" mnemonicParsing="false" onAction="#reference1FileAction"
                        prefHeight="35.0" prefWidth="110.0" text="Ajouter" GridPane.columnIndex="1"
                        GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label text="Patient :" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="addPatientButton" mnemonicParsing="false" onAction="#addPatientAction" prefHeight="35.0"
                        prefWidth="110.0" text="Ajouter" GridPane.columnIndex="1" GridPane.rowIndex="1"
                        GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="patientLabel" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="reference1FileLabel" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
                <DatePicker fx:id="inclusionDatePicker" prefHeight="31.0" prefWidth="325.0" GridPane.columnIndex="1"
                            GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </DatePicker>
                <Label text="Fichier référence PTFE 2 :" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="reference2FileButton" mnemonicParsing="false" onAction="#reference2FileAction"
                        prefHeight="35.0" prefWidth="110.0" text="Ajouter" GridPane.columnIndex="1"
                        GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="reference2FileLabel" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
            </children>
        </GridPane>
    </children>
</AnchorPane>
