<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="515.0" maxWidth="530.0" prefHeight="338.0" prefWidth="530.0"
            stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="src.controller.AddLesionController">
    <children>
        <GridPane prefHeight="332.0" prefWidth="530.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="289.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="ajouter" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Ajouter +"
                        GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <Button fx:id="annule" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Annuler"
                        GridPane.halignment="CENTER" GridPane.rowIndex="4">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <TextField fx:id="siteAnatomique" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <Label text="Site anatomique : ">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Diagnostic :" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <ChoiceBox prefHeight="31.0" prefWidth="109.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </ChoiceBox>
                <Label fx:id="checkDiag" text="Fichier de diagnostic :" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="ajoutDiag" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Ajouter"
                        GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="labelDiag" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="autre" mnemonicParsing="false" text="Autre" GridPane.columnIndex="1"
                        GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Button>
                <Label text="Fichier spectre :" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="ajoutSpectre" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Ajouter"
                        GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="labelSpectre" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
            </children>
        </GridPane>
    </children>
</AnchorPane>
