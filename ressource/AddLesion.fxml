<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="465.0" maxWidth="530.0" minHeight="465.0" prefHeight="465.0"
            prefWidth="530.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="src.controller.AddLesionController">
    <GridPane prefHeight="465.0" prefWidth="530.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="289.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <TextField fx:id="siteAnatomiqueField" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets left="10.0" right="20.0"/>
            </GridPane.margin>
        </TextField>
        <ChoiceBox fx:id="diagBox" prefHeight="31.0" prefWidth="109.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </ChoiceBox>
        <Button fx:id="otherDiagButton" mnemonicParsing="false" onAction="#otherDiagAction" text="Autre"
                GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="130.0"/>
            </GridPane.margin>
        </Button>
        <HBox alignment="CENTER_LEFT" prefHeight="93.0" prefWidth="224.0" GridPane.columnIndex="1"
              GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <Button fx:id="addPhotoSurButton" mnemonicParsing="false" onAction="#addPhotoSurAction" prefHeight="35.0"
                    prefWidth="120.0" text="Ajouter"/>
            <Label fx:id="photoSurLabel" text="Aucun" wrapText="true">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Label>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="93.0" prefWidth="224.0" GridPane.columnIndex="1"
              GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <Button fx:id="addPhotoHorsButton" mnemonicParsing="false" onAction="#addPhotoHorsAction" prefHeight="35.0"
                    prefWidth="120.0" text="Ajouter"/>
            <Label fx:id="photoHorsLabel" text="Aucun" wrapText="true">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Label>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="93.0" prefWidth="224.0" GridPane.columnIndex="1"
              GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <Button fx:id="addPhotoFixeButton" mnemonicParsing="false" onAction="#addPhotoFixeAction" prefHeight="35.0"
                    prefWidth="120.0" text="Ajouter"/>
            <Label fx:id="photoFixeLabel" text="Aucun" wrapText="true">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Label>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="93.0" prefWidth="224.0" GridPane.columnIndex="1"
              GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <Button fx:id="addDiagFileButton" mnemonicParsing="false" onAction="#addDiagFileAction" prefHeight="35.0"
                    prefWidth="120.0" text="Ajouter"/>
            <Label fx:id="diagFileLabel" text="Aucun" wrapText="true">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Label>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="93.0" prefWidth="224.0" GridPane.columnIndex="1"
              GridPane.rowIndex="6">
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
            <Button fx:id="addMoyFileButton" mnemonicParsing="false" onAction="#addMoyFileAction" prefHeight="35.0"
                    prefWidth="120.0" text="Ajouter"/>
            <Label fx:id="addMoyFileLabel" text="Aucun" wrapText="true">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </Label>
        </HBox>
        <Button fx:id="addButton" disable="true" mnemonicParsing="false" onAction="#addAction" prefHeight="50.0"
                prefWidth="150.0" text="Ajouter +" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                GridPane.rowIndex="7">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction" prefHeight="50.0"
                prefWidth="150.0" text="Annuler" GridPane.halignment="CENTER" GridPane.rowIndex="7">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Label text="Site anatomique : ">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Diagnostic :" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Photo sur :" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Photo hors :" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Photo fixé :" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Fichier pré-traité moyenné :" GridPane.rowIndex="6">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Fichier de diagnostic :" GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets left="20.0"/>
            </GridPane.margin>
        </Label>
    </GridPane>
</AnchorPane>
