<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="590.0" maxWidth="530.0" minHeight="590.0" prefHeight="590.0"
            prefWidth="530.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="src.controller.AddSiteController">
    <children>
        <GridPane prefHeight="590.0" prefWidth="530.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="63.0" minHeight="63.0" prefHeight="63.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="289.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="ajouter" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Ajouter +"
                        GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="8">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <Button fx:id="annuler" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Annuler"
                        GridPane.halignment="CENTER" GridPane.rowIndex="8">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
                <TextField fx:id="numMesure" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="orientation" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="siteAnatomique" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0" right="20.0"/>
                    </GridPane.margin>
                </TextField>
                <Label text="Site anatomique">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Numéro de mesure :" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Orientation :" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Diagnostic :" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Sain :" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <ChoiceBox fx:id="diag" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </ChoiceBox>
                <CheckBox fx:id="sain" mnemonicParsing="false" text="Oui" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </CheckBox>
                <Label text="Fichier de diagnostic :" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="addFicherDiag" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Ajouter"
                        GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="checkFichierDiag" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="aurteDiag" mnemonicParsing="false" text="Autre" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets left="170.0"/>
                    </GridPane.margin>
                </Button>
                <Label text="Fichier spectre :" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="addFichierSpectre" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Ajouter"
                        GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="checkFichierSpectre" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Fichier moyenné :" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <Button fx:id="addFichierMoy" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Ajouter"
                        GridPane.columnIndex="1" GridPane.rowIndex="7">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Button>
                <Label fx:id="checkFichierMoy" text="Aucun" GridPane.columnIndex="1" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets left="130.0"/>
                    </GridPane.margin>
                </Label>
            </children>
        </GridPane>
    </children>
</AnchorPane>
